{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "jest": {
        "setupTestFrameworkScriptFile": "./tests/setup.js"
    },
    "engine": {
        "npm": "5.7.1"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/khmy2010/YouForm.git"
    },
    "scripts": {
        "ci": "git push -u origin",
        "push-heroku": "git push heroku master",
        "push-all-complete":
            "echo \"DONE: App has been pushed to GitHub and deployed to Heroku.\"",
        "deploy":
            "npm run push-git && npm run push-heroku && npm run push-all-complete",
        "server": "nodemon --ignore 'client/' --ignore 'tests/' index.js",
        "client": "npm run start --prefix client",
        "dev":
            "concurrently  --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "build":
            "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
        "heroku-postbuild": "npm run build",
        "start": "node index.js",
        "test": "jest --forceExit --verbose"
    },
    "keywords": [],
    "author": "Ke Han",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.18.2",
        "concurrently": "^3.5.1",
        "cookie-session": "^2.0.0-beta.3",
        "express": "^4.16.3",
        "mongoose": "^5.0.10",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0"
    },
    "devDependencies": {
        "jest": "^22.4.3",
        "puppeteer": "^1.2.0",
        "uuid": "^3.2.1"
    }
}
